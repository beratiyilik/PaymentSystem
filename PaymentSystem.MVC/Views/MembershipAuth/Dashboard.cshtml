@using PaymentSystem.MVC.Models;
@using PaymentSystem.MVC.ConstantValues
@using Newtonsoft.Json;
@{
    ViewBag.Title = "Membership Dashboard";
    TokenViewModel tokenViewModel = Session[Keys.MEMBERSHIP_SESSION_KEY] != null ? JsonConvert.DeserializeObject<TokenViewModel>(Session[Keys.MEMBERSHIP_SESSION_KEY].ToString()) : null;
}

<fieldset>
    <legend> Membership Dashboard </legend>

    @if (Session[Keys.MEMBERSHIP_SESSION_KEY] != null)
    {
        
        <text>
            Welcome @tokenViewModel.UserName
        </text>
    }
</fieldset>

